@import nib

@import "ui/styl/reset"
@import "ui/styl/box-sizing-border-box-all"
@import "ui/styl/body-typography"
@import "ui/styl/responsive-helpers"
@import "ui/styl/subtle-text"
@import "ui/styl/hr"
@import "ui/styl/h1-h6"
@import "ui/styl/links-brand-colored"
@import "ui/styl/icons"
@import "ui/styl/more-links-and-buttons"
@import "ui/styl/loading-dots"
@import "ui/styl/box"
@import "ui/styl/colors"
@import "ui/styl/fonts"
@import "ui/styl/logo"
@import "ui/styl/font-smoothing"

//@import "ui/styl/code-colors"
codeBlue = #3b447f
codePurple = #633573
codeGold = #8b6253
codeTeal = #246a7b
codeRed = #b34651
codePink = #ae2f7d
codeLightPink = #bcb2c0

navigationWidth = 15rem
smallNavigationWidth = 13rem

codeOverlayWidth = 25rem
minimumEditorWidth = 25rem
editorWithCodeOverlayMinWidthQuery = "(min-width: " + (navigationWidth + codeOverlayWidth + minimumEditorWidth) + ")"
editorWithCodeOverlayMaxWidthQuery = "(max-width: " + (navigationWidth + codeOverlayWidth + minimumEditorWidth - .0625em) + ")"

codeOverlayWidthLarge = 30rem
minimumEditorWidthLarge = 45rem
editorWithCodeOverlayLargeMinWidthQuery = "(min-width: " + (navigationWidth + codeOverlayWidthLarge + minimumEditorWidthLarge) + ")"
editorWithCodeOverlayLargeMaxWidthQuery = "(max-width: " + (navigationWidth + codeOverlayWidthLarge + minimumEditorWidthLarge - .0625em) + ")"

mobileMessageMaxWidthQuery = "(max-width: " + (smallNavigationWidth + minimumEditorWidth + 5em) + ")"

::-moz-selection
  background #e0dae0

::selection
  background #e0dae0

html
  antialiasedFonts()
  font-size 16px
  background #eee

.page-loader
  position fixed
  top 0
  right 0
  bottom 0
  left navigationWidth
  background #eee
  z-index 999
  transition opacity .15s ease
  text-align center
  line-height 100vh

  @media editorWithCodeOverlayMaxWidthQuery
    left smallNavigationWidth

  .page-loader-inner
    width 20em
    height 2em
    max-width 100%
    margin auto
    text-align center

  .loading-dots
    margin-left -1.5em

  html.page-loaded &
    pointer-events none
    opacity 0

    *
      animation-play-state paused

.navigation
  position fixed
  top 0
  left 0
  bottom 0
  width navigationWidth
  padding-top 2.8em
  background-color #111
  background-image linear-gradient(#111, #1f0514)
  z-index 5

  @media editorWithCodeOverlayMaxWidthQuery
    width smallNavigationWidth

  .mobile-message
    display none

  @media mobileMessageMaxWidthQuery
    width 100%
    min-height 100vh
    position relative

    .mobile-message
      display block
      color rgba(#fff, .8)
      padding 0 2em 4em

ul.side-navigation-list
  list-style none
  padding 0
  margin 0
  line-height 1.5em
  font-size 1.1875em

  @media mobileMessageMaxWidthQuery
    display none

  > li

    &.separator
      height .0625em
      margin .375em 0

      &.large
        height 2.5em

    > a
      display block
      position relative
      cursor pointer
      padding 0.45em 2rem .45em 2rem

      &, &:hover
        color rgba(#fff, .8)
        text-decoration none

      &:hover
        background rgba(#fff, .1)

      &.active

        &, &:hover
          background #fff
          color #000

.share-target, a.attribution
  position absolute
  cursor pointer
  bottom 0
  height 60px
  background #fff
  text-align center
  border-right 1px solid #dfd1d9

  &:hover, &.share-enabled
    background #efe6eb

.share-target
  left 0
  right 50%
  line-height 60px
  font-size 1.1875em

  &, &:hover
    color #1f0514

  span
    text-transform uppercase
    font-size .7em
    letter-spacing .07em
    text-indent @letter-spacing

.share.share-element
  width navigationWidth

  @media editorWithCodeOverlayMaxWidthQuery
    width smallNavigationWidth

a.attribution
  left 50%
  right 0
  padding 1.25em

  &:not(:hover) path
    fill #1f0514

  .logo-link
    position relative
    font-size .23em
    vertical-align middle

  @media mobileMessageMaxWidthQuery
    border-right 0

.json-is-logo
  text-decoration none
  font-size 2em
  letter-spacing -.04em
  font-weight bold
  display inline-block
  line-height .8

  &, &:hover
    color #fff

  .navigation &
    margin-bottom 1em
    margin-left 2rem

.page
  position relative
  padding-left navigationWidth
  background #eee

  @media editorWithCodeOverlayMaxWidthQuery
    padding-left smallNavigationWidth

  @media mobileMessageMaxWidthQuery
    display none

.code-editor
  position relative
  overflow hidden
  width 100%
  height 100%
  z-index 10

  #editor
    font-size 16px // Note: this number is crucial to the JS
    line-height 21px // Note: this number is crucial to the JS
    height 100%
    height 100vh
    width 100%

  .code-property-path
    position fixed
    z-index 4
    bottom 0
    right 15px // later set by scrollbar width calculated with JS
    background #fff
    padding 5px
    color subtle-text
    font-size .8em

    &:empty
      display none !important

  .code-overlay
    position fixed
    z-index 5
    top 0
    right 0
    width 50%
    width codeOverlayWidth
    max-height 100vh
    overflow-y scroll
    -webkit-overflow-scrolling touch
    line-height 1.3em
    background #fff
    opacity 0
    pointer-events none

    @media editorWithCodeOverlayLargeMinWidthQuery
      width codeOverlayWidthLarge

    &[target-off-screen]
      display none !important

    &[overflow-y="true"]

      .code-overlay-content
        animation bounceInUp .8s

      &:hover:after
        opacity 0

    @media editorWithCodeOverlayMinWidthQuery
      opacity 1
      pointer-events all

    h4, h5
      font-weight bold

    h4
      font-size 1.2em
      margin-bottom 0.8333em

    h5
      font-size 1em

    pre
      line-height 1.18em
      font-size .85em
      background-color #eee
      padding 1em
      color #222
      white-space pre
      word-wrap normal
      overflow-x auto

    .code-overlay-content
      transform translate3d(0, 0, 0)
      padding 21px

      &:empty
        display none

      > *:first-child
        margin-top 0

      > *:last-child
        margin-bottom 0

      *:not(pre) > code
        font-family monospaceFonts
        font-size 0.727272em
        display inline-block
        vertical-align middle
        margin 0 .0625em
        position relative
        top -1px
        background-color #eee
        padding 0 .45em

@keyframes bounceInUp
  0%, 60%, 75%, 90%, 100%
    animation-timing-function cubic-bezier(.215, .610, .355, 1)

  0%
    opacity 0
    transform translate3d(0, 150px, 0)

  60%
    opacity 1
    transform translate3d(0, -20px, 0)

  75%
    transform translate3d(0, 10px, 0)

  90%
    transform translate3d(0, -5px, 0)

  100%
    transform translate3d(0, 0, 0)

// Ace editor styles

html .ace-tm
  color #000

  &, .ace_gutter
    background #eee

  .ace_gutter
    color #a5a5a5

    .ace_fold-widget
      transition opacity .2s ease

    &:not(:hover) .ace_fold-widget
      opacity .5

  .ace_gutter-active-line, .ace_marker-layer .ace_active-line
    background #fff

  .ace_cursor
    color #000

  .ace_marker-layer

    .ace_selection
      background #e0dae0

    .ace_step
      background rgb(102, 82, 0)

    .ace_bracket
      margin -1px 0 0 -1px
      border 1px solid #000

    .ace_selected-word
      background #fafaff
      border 1px solid #000

  .ace_fold
    background-color darken(codePink, 20%)
    border-color rgba(#000, .4)

  .ace_string
    color codeBlue

  .ace_indent-guide
    background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAACCAYAAACZgbYnAAAAEklEQVQImWPQ0FD0ZXBzd/wPAAjVAoxeSgNeAAAAAElFTkSuQmCC") right repeat-y
    opacity .2

  .ace_constant.ace_numeric
    color #5100db
    color codeBlue

  .ace_entity.ace_name.ace_function, .ace_entity.ace_other, .ace_entity.ace_other.ace_attribute-name, .ace_variable
    color codePink

// Home page styles

html.json-is-home-page
  min-height 100vh
  background-color #111
  background-image linear-gradient(#111, #1f0514)
  color rgba(#fff, .65)
  text-align center

  body
    padding 5em 1em
    min-height 100vh
    display flex
    align-items center
    justify-content center

  header a
    color #fff

  ul.docs
    padding-left 3em
    padding-right 3em
    max-width 80em
    margin-left auto
    margin-right auto
    display flex
    list-style none
    padding 0
    perspective 1000px

    > *:hover
      -webkit-transform translate3d(0, 0, -1.2rem)
      -moz-transform translate3d(0, 0, -1.2rem)
      -o-transform translate3d(0, 0, -1.2rem)
      -ms-transform translate3d(0, 0, -1.2rem)
      transform translate3d(0, 0, -1.2rem)

    @media (max-width 50em)
      display block

    > li
      flex 1
      padding 1.5em
      transition transform .3s
      display inline-block
      perspective 100rem
      max-width 100%

      > a
        display block
        height 20em
        color #fff
        user-select none
        transition color .13s ease, background-color .13s ease, transform .3s
        perspective 100rem

        > div
          border-radius 3px
          padding-top 2em
          background rgba(#fff, .06)
          transition background-color .13s ease

        &:hover
          color #000

          > div
            background #fff

        > div
          display block
          height 20em
          width 100%
          overflow hidden

          > button.more.title
            position relative
            background none
            border 0
            padding 0
            font-size 2em
            color inherit
            letter-spacing -0.025em

          > pre
            padding 1em
            text-align left
            margin 0
            white-space pre
            word-wrap normal
            overflow hidden
            font-size .8em
            color #61324b

  footer
    color rgba(#fff, .6)

    p.created
      font-size 1.2em

    a
      color #fff

// Share

@css {

.share, .share:after, .share:before, .share *, .share *:after, .share *:before {
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none
}

.share-element {
  position: absolute;
  display: none
}

  .share-element.share-open {
    display: block
  }

@font-face {
  font-family: "share-icons";
  font-style: normal;
  font-weight: normal;
  src: url(data:application/x-font-woff;charset=utf-8;base64,) format("woff")
}

.share [class^="share-icon-"]:before {
  font-family: "share-icons";
  font-style: normal;
  font-weight: normal
}

.share-icon-share:before {
  content: "\ea7d"
}

.share-icon-email:before {
  content: "\e945"
}

.share-icon-twitter:before {
  content: "\ea91"
}

.share-icon-facebook:before {
  content: "\ea8d"
}

.share-icon-github:before {
  content: "\eab1"
}

.share.share-element {
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -o-transform: translateZ(0);
  transform: translateZ(0);
  -webkit-transition: opacity 360ms;
  -moz-transition: opacity 360ms;
  -o-transition: opacity 360ms;
  transition: opacity 360ms;
  opacity: 0.999;
  z-index: 10;
}

  .share.share-element.share-open {
    display: none
  }

  .share.share-element.share-open-transitionend {
    display: block
  }

  .share.share-element.share-after-open {
    -webkit-transition: none;
    -moz-transition: none;
    -o-transition: none;
    transition: none;
    opacity: 1
  }

  .share.share-element .share-content {
    overflow: hidden;
    position: relative
  }

    .share.share-element .share-content a[class^="share-icon-"], .share.share-element .share-content a[class*=" share-icon-"] {
      padding: 0 20px 0 75px /* 64px */
    }

      .share.share-element .share-content a[class^="share-icon-"]:before, .share.share-element .share-content a[class*=" share-icon-"]:before {
        position: absolute;
        font-size: 24px;
        line-height: 30px;
        height: 30px;
        width: 30px;
        top: 0;
        bottom: 0;
        left: 30px; /* 20px */
        margin: auto;
        text-align: center;
        speak: none
      }

      .share.share-element .share-content .share-icon-twitter:before {
        color: #55acee
      }

      .share.share-element .share-content .share-icon-facebook:before {
        color: #3b5998
      }

      .share.share-element .share-content .share-icon-email:before {
        color: #efe6eb
      }

    .share.share-element .share-content ul {
      position: relative;
      display: block;
      list-style-type: none;
      margin: 0;
      padding: 0
    }

      .share.share-element .share-content ul li {
        position: relative;
        display: block
      }

        .share.share-element .share-content ul li a {
          width: 100%;
          background: #2f1c27;
          color: rgba(255, 255, 255, .8);
          line-height: 60px;
          height: 60px;
          padding: 0 30px 0 75px;
          display: block;
          cursor: pointer;
          -webkit-backface-visibility: hidden;
          -webkit-touch-callout: none;
          font-size: 1.1875em;
          font-weight: normal;
          -webkit-transform: translate3d(0, 0, 0);
          -moz-transform: translate3d(0, 0, 0);
          -ms-transform: translate3d(0, 0, 0);
          -o-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0)
        }

          .share.share-element .share-content ul li a:hover {
            background: #27131e;
            color: #fff
          }

        .share.share-element .share-content ul li:nth-child(1) a {
          -webkit-transform: translate3d(0, -60px, 0);
          -moz-transform: translate3d(0, -60px, 0);
          -ms-transform: translate3d(0, -60px, 0);
          -o-transform: translate3d(0, -60px, 0);
          transform: translate3d(0, -60px, 0);
          -webkit-transition: -webkit-transform 240ms ease;
          -webkit-transition-delay: 0ms;
          -moz-transition: -moz-transform 240ms ease 0ms;
          -o-transition: -o-transform 240ms ease 0ms;
          transition: transform 240ms ease 0ms
        }

        .share.share-element .share-content ul li:nth-child(2) a {
          -webkit-transform: translate3d(0, -120px, 0);
          -moz-transform: translate3d(0, -120px, 0);
          -ms-transform: translate3d(0, -120px, 0);
          -o-transform: translate3d(0, -120px, 0);
          transform: translate3d(0, -120px, 0);
          -webkit-transition: -webkit-transform 240ms ease;
          -webkit-transition-delay: 60ms;
          -moz-transition: -moz-transform 240ms ease 60ms;
          -o-transition: -o-transform 240ms ease 60ms;
          transition: transform 240ms ease 60ms
        }

        .share.share-element .share-content ul li:nth-child(3) a {
          -webkit-transform: translate3d(0, -180px, 0);
          -moz-transform: translate3d(0, -180px, 0);
          -ms-transform: translate3d(0, -180px, 0);
          -o-transform: translate3d(0, -180px, 0);
          transform: translate3d(0, -180px, 0);
          -webkit-transition: -webkit-transform 240ms ease;
          -webkit-transition-delay: 120ms;
          -moz-transition: -moz-transform 240ms ease 120ms;
          -o-transition: -o-transform 240ms ease 120ms;
          transition: transform 240ms ease 120ms
        }

  .share.share-element.share-after-open .share-content ul li:nth-child(1) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 120ms;
    -moz-transition: -moz-transform 240ms ease 120ms;
    -o-transition: -o-transform 240ms ease 120ms;
    transition: transform 240ms ease 120ms
  }

  .share.share-element.share-after-open .share-content ul li:nth-child(2) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 60ms;
    -moz-transition: -moz-transform 240ms ease 60ms;
    -o-transition: -o-transform 240ms ease 60ms;
    transition: transform 240ms ease 60ms
  }

  .share.share-element.share-after-open .share-content ul li:nth-child(3) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 0ms;
    -moz-transition: -moz-transform 240ms ease 0ms;
    -o-transition: -o-transform 240ms ease 0ms;
    transition: transform 240ms ease 0ms
  }

  .share.share-element.share-element-attached-bottom .share-content ul li:nth-child(1) a {
    -webkit-transform: translate3d(0, 180px, 0);
    -moz-transform: translate3d(0, 180px, 0);
    -ms-transform: translate3d(0, 180px, 0);
    -o-transform: translate3d(0, 180px, 0);
    transform: translate3d(0, 180px, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 120ms;
    -moz-transition: -moz-transform 240ms ease 120ms;
    -o-transition: -o-transform 240ms ease 120ms;
    transition: transform 240ms ease 120ms
  }

  .share.share-element.share-element-attached-bottom .share-content ul li:nth-child(2) a {
    -webkit-transform: translate3d(0, 120px, 0);
    -moz-transform: translate3d(0, 120px, 0);
    -ms-transform: translate3d(0, 120px, 0);
    -o-transform: translate3d(0, 120px, 0);
    transform: translate3d(0, 120px, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 60ms;
    -moz-transition: -moz-transform 240ms ease 60ms;
    -o-transition: -o-transform 240ms ease 60ms;
    transition: transform 240ms ease 60ms
  }

  .share.share-element.share-element-attached-bottom .share-content ul li:nth-child(3) a {
    -webkit-transform: translate3d(0, 60px, 0);
    -moz-transform: translate3d(0, 60px, 0);
    -ms-transform: translate3d(0, 60px, 0);
    -o-transform: translate3d(0, 60px, 0);
    transform: translate3d(0, 60px, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 0ms;
    -moz-transition: -moz-transform 240ms ease 0ms;
    -o-transition: -o-transform 240ms ease 0ms;
    transition: transform 240ms ease 0ms
  }

  .share.share-element.share-element-attached-bottom.share-after-open .share-content ul li:nth-child(1) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 0ms;
    -moz-transition: -moz-transform 240ms ease 0ms;
    -o-transition: -o-transform 240ms ease 0ms;
    transition: transform 240ms ease 0ms
  }

  .share.share-element.share-element-attached-bottom.share-after-open .share-content ul li:nth-child(2) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 60ms;
    -moz-transition: -moz-transform 240ms ease 60ms;
    -o-transition: -o-transform 240ms ease 60ms;
    transition: transform 240ms ease 60ms
  }

  .share.share-element.share-element-attached-bottom.share-after-open .share-content ul li:nth-child(3) a {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition: -webkit-transform 240ms ease;
    -webkit-transition-delay: 120ms;
    -moz-transition: -moz-transform 240ms ease 120ms;
    -o-transition: -o-transform 240ms ease 120ms;
    transition: transform 240ms ease 120ms
  }

}